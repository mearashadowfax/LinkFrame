{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "src/components/common/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
      "@favicon/*": [
        "src/favicon/*"
      ],
    }
  }
}